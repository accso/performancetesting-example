<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-run</actionName>
            <displayName>run</displayName>
            <goals>
                <goal>spring-boot:run</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-gatling</actionName>
            <displayName>gatling</displayName>
            <goals>
                <goal>gatling:execute</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-run-debug</actionName>
            <displayName>run-debug</displayName>
            <goals>
                <goal>spring-boot:run</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=8989 -classpath %classpath ${packageClassName}</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
    </actions>
